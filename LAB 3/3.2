close all;
clear;
clc;

R1 = 16;
R2 = 34;
R3 = 100;
R0 = 423;
R4 = 10;

E1 = 7;
E2 = 1;
E3 = 12;

A =[R1+R2 ,  -R2  ,   0   ;
    -R2   , R2+R3 ,  -R3  ;
     0    ,  -R3  , R3+R0+R4;];
b =[ E1-E2 ;
     E2-E3 ;
      E3   ;];
x1 = inv(A)*b;%Macierz odwrotna do a razy wektor
x2 = pinv(A)*b; % (A^T * A)^(-1) * A^T
x3 = A\b;  % % minimaliacja bledu sredniokwadratowego
%Cramer
for k =1:length(b)
    Ak =A;
    Ak(:,k)= b;
    x4(k) = det(Ak)/det(A);
end
x4 = x4.';
[x1,x2,x3,x4]

